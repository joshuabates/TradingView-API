{
  "name": "@mathieuc/tradingview",
  "version": "3.5.1",
  "description": "Tradingview instant stocks API, indicator alerts, trading bot, and more !",
  "main": "main.js",
  "browser": "dist/tradingview-api.browser.js",
  "exports": {
    ".": {
      "node": "./main.js",
      "browser": "./browser.js",
      "import": {
        "node": "./main.js",
        "default": "./browser.js"
      },
      "require": "./main.js"
    },
    "./browser": "./browser.js",
    "./vite": "./vite-browser.js",
    "./node": "./main.js",
    "./dist/*": "./dist/*"
  },
  "scripts": {
    "test": "vitest",
    "example": "node --env-file=.env",
    "example:dev": "nodemon --env-file=.env",
    "build": "npm run build:browser",
    "build:browser": "webpack --config webpack.config.js",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mathieu2301/TradingView-API.git"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "tradingwiew",
    "market",
    "stocks",
    "crypto",
    "forex",
    "indicator",
    "bitcoin",
    "api"
  ],
  "author": "Mathieu Colmon",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.5.0",
    "jszip": "^3.7.1",
    "ws": "^7.4.3"
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/eslint-parser": "^7.15.7",
    "@babel/plugin-transform-runtime": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@mathieuc/console": "^1.0.1",
    "babel-loader": "^9.1.2",
    "dotenv": "^16.3.1",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "vitest": "^0.31.4",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4"
  }
}
